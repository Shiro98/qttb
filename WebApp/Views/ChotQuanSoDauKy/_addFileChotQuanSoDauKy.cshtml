<div class="modal-header">
    <button type="button" class="bootbox-close-button close" ng-click="cancel()">×</button>
    <h4 class="modal-title">Update file</h4>
</div>
<div class="modal-body">
    <div class="bootbox-body">
        <form class="bootbox-form" id="formSubmit">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        @*<input type="text" style="display: none" class="form-control" ng-model="model.FileUrl">*@
                        <input type="file" ui-chose-file onchange="angular.element(this).scope().SelectFile(event)" class="custom-file-input" />
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-primary" ng-click="Upload()">Upload</button>
    <button type="button" class="btn btn-default" ng-click="cancel()">Hủy</button>
</div>